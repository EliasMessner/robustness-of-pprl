{
  "base_dataset": "data/2021_NCVR_Panse_001/dataset_ncvr_dirty.csv",
  "col_names": ["sourceID", "globalID", "localID", "FIRSTNAME", "MIDDLENAME", "LASTNAME", "YEAROFBIRTH", "PLACEOFBIRTH", "COUNTRY", "CITY", "PLZ", "STREET", "GENDER", "ETHNIC", "RACE"],
  "source_id_col_name": "sourceID",
  "global_id_col_name": "globalID",
  "desc": "Variation of gender. Selection by male/female and different distributions of these",
  "variations":[
    {
      "desc": "All genders with downsampling",
      "params": {
        "subset_selection": "ATTRIBUTE_VALUE",
        "column": "GENDER",
        "equals": null,
        "downsampling": "TO_MIN_GROUP_SIZE"
      },
      "replacements": {
        "equals": ["M", "F", "U"]
      }
    },
    {
      "desc": "Genders M and F with downsampling",
      "params": {
        "subset_selection": "ATTRIBUTE_VALUE",
        "column": "GENDER",
        "equals": null,
        "downsampling": "TO_MIN_GROUP_SIZE"
      },
      "replacements": {
        "equals": ["M", "F"]
      }
    },
    {
      "desc": "Different ratios of M/F, with downsampling",
      "params": {
        "subset_selection": "ATTRIBUTE_VALUE",
        "column": "GENDER",
        "dist": null,
        "downsampling": "TO_MIN_GROUP_SIZE"
      },
      "replacements": {
        "dist": [
          {"M": 0.2, "F": 0.8},
          {"M": 0.4, "F": 0.6},
          {"M": 0.6, "F": 0.4},
          {"M": 0.8, "F": 0.2}
        ]
      }
    }
  ]
}