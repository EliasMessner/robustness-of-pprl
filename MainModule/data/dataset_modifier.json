{
  "base_dataset": "data/2021_NCVR_Panse_001/dataset_ncvr_dirty.csv",
  "col_names": ["sourceID", "globalID", "localID", "FIRSTNAME", "MIDDLENAME", "LASTNAME", "YEAROFBIRTH", "PLACEOFBIRTH", "COUNTRY", "CITY", "PLZ", "STREET", "GENDER", "ETHNIC", "RACE"],
  "source_id_col_name": "sourceID",
  "global_id_col_name": "globalID",
  "variations":[
    {
      "params": {
        "subset_selection":  "RANDOM",
        "seed":  1,
        "size": 10000,
        "overlap": 0.1
      },
      "replacements": {
        "size": [2000, 4000, 20000],
        "overlap": [0.1, 0.2, 0.3]
      }
    },
    {
      "params": {
        "subset_selection":  "GENDER",
        "allowed": []
      },
      "replacements":  {
        "allowed": [["M"], ["F"], ["M", "U"], ["F", "U"]]
      }
    },
    {
      "params": {
        "subset_selection": "PLZ",
        "digits": 1,
        "equal": 0
      },
      "replacements": {
        "equal": [1, 2, 3, 4, 5, 6, 7, 8, 9]
      },
      "include_default": true
    },
    {
      "params": {
        "subset_selection": "AGE",
        "range": [0, 19]
      },
      "replacements": {
        "range": [[20, 39], [40, 59], [60, 79], [80, 120]]
      },
      "include_default": true
    }
  ]
}