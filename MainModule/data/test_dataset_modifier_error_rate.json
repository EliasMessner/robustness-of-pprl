{
  "base_dataset": "data/2021_NCVR_Panse_001/dataset_ncvr_dirty.csv",
  "col_names": ["sourceID", "globalID", "localID", "FIRSTNAME", "MIDDLENAME", "LASTNAME", "YEAROFBIRTH", "PLACEOFBIRTH", "COUNTRY", "CITY", "PLZ", "STREET", "GENDER", "ETHNIC", "RACE"],
  "source_id_col_name": "sourceID",
  "global_id_col_name": "globalID",
  "variations": [
    {
      "desc": "Different error rate ranges, without downsampling.",
      "params": {
        "subset_selection": "ERROR_RATE",
        "measure": "count_errors",
        "range": null,
        "downsampling": null,
        "preserve_overlap": true,
        "seed": 42
      },
      "replacements": {
        "range": [[0, 0], [1, 3], [3, 11]]
      }
    },
    {
      "desc": "Different error rate ranges, with downsampling to min group size.",
      "params": {
        "subset_selection": "ERROR_RATE",
        "measure": "count_errors",
        "range": null,
        "downsampling": "TO_MIN_GROUP_SIZE",
        "preserve_overlap": false
      },
      "replacements": {
        "range": [[0, 0], [1, 3], [3, 11]]
      }
    }
  ]
}