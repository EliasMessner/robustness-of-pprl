{
  "base_dataset": "data/2023_NCVR_Panse_1M/records.csv",
  "col_names": ["sourceID", "globalID", "localID", "FIRSTNAME", "MIDDLENAME", "LASTNAME", "YEAROFBIRTH", "PLACEOFBIRTH", "COUNTRY", "CITY", "PLZ", "STREET", "GENDER", "ETHNIC", "RACE"],
  "source_id_col_name": "sourceID",
  "global_id_col_name": "globalID",
  "desc": "Varying lengths for FIRSTNAME, LASTNAME, and CITY, resulting in subsets of each 100k records",
  "variations": [
    {
      "desc": "Length of FIRSTNAME in [<5, 5, 6, 7, >7], downsampling on",
      "params": {
        "subset_selection": "ATTRIBUTE_VALUE",
        "column": null,
        "length": null,
        "downsampling": 100000,
        "preserve_overlap_in_downsampling": true
      },
      "replacements": {
        "length": [[1, 4], [5, 5], [6, 6], [7, 7], [8, 30]],
        "column": ["FIRSTNAME"]
      }
    },
    {
      "desc": "Length of LASTNAME in [<6, 6, 7, >7], downsampling on",
      "params": {
        "subset_selection": "ATTRIBUTE_VALUE",
        "column": null,
        "length": null,
        "downsampling": 100000,
        "preserve_overlap_in_downsampling": true
      },
      "replacements": {
        "length": [[1, 5], [6, 6], [7, 7], [8, 30]],
        "column": ["LASTNAME"]
      }
    },
    {
      "desc": "Length of CITY in [<7, 7-8, 9, >9], downsampling on",
      "params": {
        "subset_selection": "ATTRIBUTE_VALUE",
        "column": "CITY",
        "length": null,
        "downsampling": 100000,
        "preserve_overlap_in_downsampling": true
      },
      "replacements": {
        "length": [[0, 6], [7, 8], [9, 9], [10, 11]]
      }
    }
  ]
}