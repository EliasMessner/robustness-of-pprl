{
  "base_dataset": "data/2023_NCVR_Panse_1M/records.csv",
  "col_names": ["sourceID", "globalID", "localID", "FIRSTNAME", "MIDDLENAME", "LASTNAME", "YEAROFBIRTH", "PLACEOFBIRTH", "COUNTRY", "CITY", "PLZ", "STREET", "GENDER", "ETHNIC", "RACE"],
  "source_id_col_name": "sourceID",
  "global_id_col_name": "globalID",
  "desc": "Division by first 2 or 3 digits of PLZ, downsampling to 200k",
  "variations": [
    {
      "desc": "Divided by first 2 digits of PLZ. \nDownsampling=200k",
      "params": {
        "subset_selection": "PLZ",
        "digits": 2,
        "equals": null,
        "downsampling": 200000
      },
      "replacements": {
        "equals": [0, 99]
      },
      "as_range": true
    },
    {
      "desc": "Divided by first 3 digits of PLZ. \nDownsampling=200k",
      "params": {
        "subset_selection": "PLZ",
        "digits": 3,
        "equals": null,
        "downsampling": 200000
      },
      "replacements": {
        "equals": [0, 999]
      },
      "as_range": true
    }
  ]
}
